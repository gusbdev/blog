      <hr class="my-4" />
      <p>Simple blog, made with NODEJS, EJS.</p>
      <p class="lead">
        <a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a>
      </p>
    </div>
    <hr />
    <% articles.forEach(article => { %>
    <div class="card">
      <div class="card-header"><%= article.title %></div>
      <div class="card-body">
        <a href="" class="btn btn-success">Ler artigo</a>
      </div>
    </div>
    <br />
    <% }) %>
  </div>
</body>
